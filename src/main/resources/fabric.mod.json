{
	"schemaVersion": 1,
	"id": "collis",
	"version": "${version}",
	"name": "Collis",
	"description": "A library for creating server-side camera animation tracks.",
	"authors": [
		"Andante"
	],
	"contact": {
		"sources": "https://github.com/mcbrawls/collis"
	},
	"license": "MIT",
	"icon": "assets/collis/icon.png",
	"environment": "*",
	"mixins": [
		"collis.mixins.json"
	],
	"entrypoints": {
		"main": [
			{
				"adapter": "kotlin",
				"value": "net.mcbrawls.collis.Collis::onInitialize"
			}
		]
	},
	"accessWidener": "collis.accesswidener",
	"depends": {
		"fabricloader": "*",
		"minecraft": "~1.21.3",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}
